<h1>Listing votes</h1>

<table>
  <tr>
    <th>Sentiment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @votes.each do |vote| %>
  <tr>
    <div><%= vote.sentiment %></div>
    <div><%= link_to 'Show', vote %></div>
    <div><%= link_to 'Edit', edit_vote_path(vote) %></div>
    <div><%= link_to 'Destroy', vote, method: :delete, data: { confirm: 'Are you sure?' } %></div>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Vote', new_vote_path %>
