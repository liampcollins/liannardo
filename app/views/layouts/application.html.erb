<!DOCTYPE html>
<html>
<head>
  <title>Cheerups</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user %>
      <%= current_user.email %>
      <%= link_to 'logout', destroy_user_session_path, method: :delete %>
      <%= link_to 'View Feed', posts_path  %>

      <img id="image" src="<%= current_user.profile_picture %>" />
      
    <% else %>
      <%= link_to 'sign in', new_user_session_path %>
      <%= link_to 'sign up', new_user_registration_path %>
      <%= link_to "Login with Google", user_omniauth_authorize_path(:google_oauth2)%>
      <%= link_to "Login with Facebook", user_omniauth_authorize_path(:facebook)%>

    <% end %>

  
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>


<%= yield %>

</body>
</html>
